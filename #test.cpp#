#include <string>
#include <iostream>
#include <cmath>
#include <stdio.h>

void foo();

struct myStruct {
        myStruct() {
                str = "hello struct";
        }
        int a;
        std::string str;
        int foo(int a) {
                return a++;
        }
        int averyveryverylongvariablename;
        int averyverysortalongting;
        int averyverysolrtalongting;
        int alveryverysortalongting;
};


int main() {
        foo();

        myStruct s;

        std::string str = "hello";

        std::cout << s.str << std::endl;

        s.foo(5);

        int f = s.averyveryverylongvariablename;

        str.assign(8, 'd');

        s.foo(15);

        str.append(34, 9);

        return f;
}

void foo() {
        std::cout << "hello, this is foo()" << std::endl;
        std::cout << "hello, this is foo()" << std::endl;

        return;
}
